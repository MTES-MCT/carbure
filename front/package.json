{
  "name": "carbure-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.25.3",
    "@iframe-resizer/react": "^5.4.7",
    "@sentry/react": "^9.31.0",
    "@sentry/vite-plugin": "^3.5.0",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.19",
    "@types/node": "^24.0.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.2.1",
    "i18next-http-backend": "^3.0.2",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "msw": "^2.10.2",
    "openapi-fetch": "^0.14.0",
    "ramda": "^0.31.3",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.3",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.8.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.0",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/react": "^16.3.0",
    "@types/ramda": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "chromatic": "^11.11.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.11.3",
    "i18next-parser": "^9.3.0",
    "jsdom": "^27.0.0",
    "msw-storybook-addon": "^2.0.5",
    "openapi-msw": "^2.0.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.0",
    "storybook": "^8.6.14",
    "storybook-addon-mock-date": "^0.6.0",
    "storybook-addon-remix-react-router": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript-plugin-css-modules": "^5.2.0",
    "vitest": "^3.2.4",
    "yaml": "^2.8.0"
  },
  "scripts": {
    "start-server": "cd ../ && docker-compose up -d && cd front && npm run open",
    "open": "open http://carbure.local:8090/",
    "stop-server": "docker-compose down",
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "translate": "i18next",
    "translate-missing": "node scripts/translate-missing.js",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}' && prettier --check src/**/*.css",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx,json}' --fix && prettier --write src/**/*.css",
    "prestart": "react-dsfr update-icons",
    "prestorybook": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --test",
    "chromatic": "chromatic --exit-zero-on-changes",
    "generate-api": "node --loader ts-node/esm scripts/overrides-api-schema.ts && node --loader ts-node/esm scripts/transform-schema.ts",
    "check-diff": "git diff --quiet --exit-code ../api-schema.yaml || (echo 'Attention, le schéma a évolué depuis la dernière génération. Veuillez vérifier les différences et mettre à jour le schéma si nécessaire.')",
    "check-types": "tsc --noEmit",
    "generate-and-check-types": "npm run generate-api && npm run check-diff && npm run check-types"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
