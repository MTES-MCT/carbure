{
  "name": "carbure-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.9.8",
    "@sentry/react": "^8.30.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.7",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "i18next": "^22.4.11",
    "i18next-http-backend": "^2.1.1",
    "iframe-resizer-react": "^1.1.0",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "msw": "^2.4.9",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.8.2",
    "react-scripts": "^5.0.1",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    }
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "i18next-parser": "^7.7.0",
    "jest-environment-jsdom": "^29.2.5",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "undici": "^6.19.8"
  },
  "scripts": {
    "start-server": "cd ../ && docker-compose up -d && cd front && npm run open",
    "open": "open http://carbure.local:8090/",
    "stop-server": "docker-compose down",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "translate": "i18next",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}' && prettier --check src/**/*.css",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx,json}' --fix && prettier --write src/**/*.css",
    "postinstall": "copy-dsfr-to-public --publicDir ./public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
